{
  "name": "rapidprotokit",
  "version": "0.0.1",
  "description": "Opinionated boilerplate and suite of components for quickly creating React based prototypes.",
  "author": "Richard Benjamin",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "ui",
    "prototyping"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rbenj/rapidprotokit.git"
  },
  "homepage": "https://github.com/rbenj/rapidprotokit#readme",
  "bugs": {
    "url": "https://github.com/rbenj/rapidprotokit/issues"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "cypress": "cypress run"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dexie": "^4.2.1",
    "lucide-react": "^0.561.0",
    "normalize.css": "^8.0.1",
    "rapidprotokit-schema-mold": "github:rbenj/rapidprotokit-schema-mold",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "cypress": "^15.2.0",
    "eslint": "^9.27.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
